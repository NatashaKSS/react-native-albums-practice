{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "classes": true,
  },
  rules: {
    'react/jsx-filename-extension': ['error', { extensions: ['.js', '.jsx'] }],
    'function-paren-newline': ['error', { "minItems": 3 }],
    'global-require': 'off',
  },
  globals: {
    "fetch": true
  }
}
